<body>
  <div class="container">
    <button id="redirect-btn">pencet dongzzz</button>
  </div>
  <script>
    // Pilih tombol dengan id "redirect-btn"
    const redirectButton = document.getElementById('redirect-btn');

    // Tambahkan event listener untuk klik
    redirectButton.addEventListener('click', () => {
      // Arahkan ke tautan Instagram Stories
      window.location.href = 'https://www.instagram.com/stories/ti_uinma/3540115785807135250?igsh=bGYxbGNyZ21jMWl0';
    });
  </script>
